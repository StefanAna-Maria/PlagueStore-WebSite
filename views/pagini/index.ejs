            <section id="galerie-statica">
                <h2>Galerie</h2>
                <div id="grid-galerie">
                    <% 
                    var d = new Date();
                    var ora_curenta = d.getHours() * 60 + d.getMinutes(); // Convertim ora curentă în minute
            
                    function inIntervalOrar(interval, ora_curenta) {
                        var [start, end] = interval.split("-");
                        var [startHour, startMinute] = start.split(":").map(Number);
                        var [endHour, endMinute] = end.split(":").map(Number);
            
                        var startTime = startHour * 60 + startMinute;
                        var endTime = endHour * 60 + endMinute;
            
                        return ora_curenta >= startTime && ora_curenta <= endTime;
                    }
            
                    for (let imag of locals.imagini) { 
                        if (imag?.timp && inIntervalOrar(imag.timp, ora_curenta)) { 
                    %>
                        <figure>
                            <picture>
                                <source srcset="<%= imag.fisier_mediu %>" media="(max-width:1200px)" />
                                <img src="<%= imag.fisier %>" alt="<%= imag.titlu %>">
                            </picture>
                            <figcaption>
                                <strong><%= imag.titlu %></strong><br>
                                
                            </figcaption>
                        </figure>
                    <% 
                        } 
                    } 
                    %>
                </div>
            </section>

